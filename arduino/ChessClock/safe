/*
*/
const int player_1 = 14;
const int player_2 = 0;
void button_setup(){
  pinMode(player_1, INPUT_PULLUP);
  pinMode(player_2, INPUT_PULLUP);
}

void setup(){
  //display_setup();
  button_setup();
  Serial.begin(115200);
}

long counter_seconds0 = 90L;
long counter_seconds1 = 234L;
/*
*/
void button_loop(){
  Serial.print(digitalRead(player_1));
  Serial.println(digitalRead(player_2));
}

void loop(){
  //display_loop();
  delay(100);
  button_loop();
}
